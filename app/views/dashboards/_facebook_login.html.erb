<%- if devise_mapping.omniauthable? && !user_signed_in?%>
        <%- resource_class.omniauth_providers.each do |provider| %>
        <%#= link_to "Sign in with #{provider.to_s.titleize}", omniauth_authorize_path(resource_name, provider), id: "sign_in" %>
        <%= link_to omniauth_authorize_path(resource_name,provider),id: "popup" do %>
          <div class="facebook text-center" >
              <div class="spacer"><i class="fa-facebook btn btn-facebook">Login with Facebook</i></div>
          </div>
          <% end -%>
        <% end -%>
      <% end -%>

