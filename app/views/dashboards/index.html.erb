html,body,section,.page {
    width: 100%;
    height: 100%;
    margin: 0;
    padding: 0;
    transition: all 1s cubic-bezier(.5,-0.005,0.2,1) !important;
    -webkit-transition: all 1s cubic-bezier(.5,-0.005,0.2,1) !important;
    color: #fff;
    background: #000;
/* Home background color */
    overflow-x: hidden;
    overflow-y: scroll;
}

section {
    width: 400%;
}

.page {
    position: absolute;
}

.inside {
    display: inline-block;
}

/* close commented backslash hack */
/* 
> Position and background color of individual pages
> p1 = Home
> p2 = Events
> p3 = Messages
*/
#p1 {
    left: 7%;
    position: fixed;
}

#p2 {
    left: 100%;
    background: #b5b5b7;
    position: fixed;
}

#p3 {
    left: 200%;
    background: #ffd006;
    position: fixed;
}

/*added  ***commented out by SK*/
#event-page-style {
    padding-right: 13%;
/*padding-left: 70px; */
}

ul {
    position: fixed;
    z-index: 1;
}

#t1:target .page#p1 {
    transform: translateX(0);
}

#t2:target .page#p2 {
    transform: translateX(-90%);
}

#t3:target .page#p3 {
    transform: translateX(-190%);
}

/* Background color during page transitions */
#t2:target .facebook,#t3:target .facebook {
    background-color: Transparent;
    background-repeat: no-repeat;
    border: none;
    cursor: pointer;
    overflow: hidden;
    outline: none;
}

#t2:target .page#p1,#t3:target .page#p1 {
    background: #000;
}

/* Screen blur during page transitions */
#t2:target .page#p1 .icon,#t3:target .page#p1 .icon {
    -webkit-filter: blur(3px);
    filter: blur(3px);
}

.icon {
    color: #fff;
    font-size: 32px;
    display: block;
}

ul .icon:hover {
    opacity: .6;
}

/* Don't worry about this, will delete later */
#p1 .icon .title {
    margin-bottom: 20px;
}

/* Icons enlarge on hover */
#t2:target ul .icon,#t3:target ul .icon {
    transform: scale(.6);
    transition-delay: .25s;
}

/* Icons enlarge depending on what page youre on */
#t2:target #dos {
    transform: scale(1.2) !important;
}

#t3:target #tres {
    transform: scale(1.2) !important;
}

ul {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
/*right: 0;*/
    margin: auto;
    height: 280px;
    width: 10%;
    padding: 0;
    text-align: center;
}

ul li {
