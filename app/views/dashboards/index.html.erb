}

.hint a {
    color: #EFFF06;
    transition: all 250ms easet-out;
}

.hint a:hover {
    color: #FFF;
}

.page .icon {
    position: absolute;
    top: 0;
    bottom: 0;
    right: 10%;
    left: 0;
    width: 270px;
    height: 170px;
    margin: auto;
    text-align: center;
    font-size: 80px;
    transform: translateX(360%);
    line-height: 1.3;
}

.page#p1 .icon {
    height: 220px;
}

.page#p1 .icon {
    transform: translateX(0) !important;
    transition-delay: 1s;
}

#t2:target .page#p2 .icon {
    transform: translateX(0) !important;
    transition-delay: 1s;
}

#t3:target .page#p3 .icon {
    transform: translateX(0) !important;
    transition-delay: 1s;
}

