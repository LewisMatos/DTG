    <style>
    * {
    transition: all 200ms ease-out;
    -webkit-transition: all 200ms ease-out;
}

html, body, section, .page {
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  transition: all 1s cubic-bezier(.5, -0.005, 0.2, 1) !important;
  -webkit-transition: all 1s cubic-bezier(.5, -0.005, 0.2, 1) !important;
  color: #fff;
  background: black; /* Home background color */
  overflow-x: hidden;
  overflow-y: scroll;
}

section {
  width:400%;  
}

.page {
  position: absolute;
}


/* 
> Position and background color of individual pages
> p1 = Home
> p2 = Events
> p3 = Messages
*/

#p1 {
  left: 10%;
}

#p2 {
  left: 100%;
  background: white;
}

#p3 {
  left: 200%;
  background: #ffd006;
}


/*added*/
#event-page-style {
  padding-right: 150px;
}

ul {
 position: fixed; 
 z-index: 1;
}
#t1:target .page#p1 {
    transform: translateX(0);
}
#t2:target .page#p2 {
    transform: translateX(-90%);
}
#t3:target .page#p3 {
    transform: translateX(-190%);
}

/* Background color during page transitions */
#t2:target .page#p1, 
#t3:target .page#p1 {
  background: black;
}

/* Screen blur during page transitions */
#t2:target .page#p1 .icon, 
#t3:target .page#p1 .icon {
  -webkit-filter: blur(3px);
  filter: blur(3px);
}

.icon {
  color: #fff;
  font-size: 32px;
  display: block;
}

ul .icon:hover {
  opacity: 0.6;
}

/* Don't worry about this, will delete later */
#p1 .icon .title {
  margin-bottom: 20px;
}

/* Icons enlarge on hover */
#t2:target ul .icon,
#t3:target ul .icon {
  transform: scale(.6); 
  transition-delay: .25s;
}

/* Icons enlarge depending on what page your on */
#t2:target #dos {
  transform: scale(1.2) !important;
}

#t3:target #tres {
  transform: scale(1.2) !important;
}


ul {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  /*right: 0;*/
  margin: auto;
  height: 280px;
  width: 10%;
  padding: 0;
  text-align: center;
 }

ul li {
  margin: 30px 0;
}

a {
  text-decoration: none;
  font-family: open sans, sans-serif;
}
  
.title, .hint {
  font-family: open sans, sans-serif;
  display: block;
}

.title {
  font-size: 38px;
}

.hint {
  font-size: 13px;
}

.hint a {
  color: #EFFF06;
  transition: all 250ms easet-out;
}

.hint a:hover {
  color: #FFF;
}

.page .icon {
  position: absolute;
  top: 0;
  bottom: 0;
  right: 10%;
  left: 0;
  width: 270px;
  height: 170px;
  margin: auto;
  text-align: center;
  font-size: 80px;
  transform: translateX(360%);
  line-height: 1.3;
}

.page#p1 .icon {
  height: 220px;
}

.page#p1 .icon {
  transform: translateX(0) !important;
}

#t2:target .page#p2 .icon {
  transform: translateX(0) !important;
  transition-delay: 1s;
}

#t3:target .page#p3 .icon {
  transform: translateX(0) !important;
  transition-delay: 1s;
}


.credit{
  position: absolute;
  bottom: 0;
  width: 100%;
  text-align: center;
  font-size: 10px;
}

.credit a {
  color: #fff;
}

.homepage-event-image { 
  padding: 20px;
  }

.event-link {
  display: block;
  height: 250px;
  width: 250px;
  -webkit-transition: -webkit-transform .15s linear;
  -webkit-box-shadow: 0 3px 6px rgba(0,0,0,.25);
  -moz-box-shadow: 0 3px 6px rgba(0,0,0,.25);-webkit-box-shadow: 0 3px 6px rgba(0,0,0,.5);
  -moz-box-shadow: 0 3px 6px rgba(0,0,0,.5);
}

.somelogo {
  padding: 0;
  margin: 0;
}

.home-page-background {
  background:url('/assets/dtglogo2.jpg')
  center no-repeat;
  background-size: cover;
}

.one-event-image {
}

body.modal-open, 
.modal-open .navbar-fixed-top, 
.modal-open .navbar-fixed-bottom {
  margin-right: 0;
}

.modal {
  left: 10%;
  top: 10%;
  bottom: auto;
  right: auto;
  z-index: 1050;
  padding: 0;
  width: 70%;
/*  margin-left: -340px;
*/  background-color: #ffffff;
  border: 1px solid #999999;
  border: 1px solid rgba(0, 0, 0, 0.2);
  border-radius: 6px;
  -webkit-box-shadow: 0 3px 9px rgba(0, 0, 0, 0.5);
  box-shadow: 0 3px 9px rgba(0, 0, 0, 0.5);
  background-clip: padding-box;
}
.modal-title {
  color: black;
}
.modal-event-title {
  text-align: center;
}

.modal.container {
  max-width: none;
}

.modal-backdrop {
  display:none;
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 0;
}


   </style>


  <div class="ct" id="t1">
    <div class="ct" id="t2">
      <div class="ct" id="t3">
        <div class="ct" id="t4">


          <section>  
             <!-- Left navigation bar. Icons that use anchors to pull in page into view (#t2 = Events page) -->
             <ul>

               <ul class="hmenu">
                <%= render 'devise/menu/registration_items' %>
                <%= render 'devise/menu/login_items' %>
               </ul>

               <a href="#t1"><li class="icon fa fa-home" id="uno"><span class="hint"> Home </span></li></a>
               <a href="#t2"><li class="icon fa fa-thumb-tack" id="dos"><span class="hint"> Events </span></li></a>
               <a href="#t3"><li class="icon fa fa-comments" id="tres"><span class="hint"> Messages </span></li></a>
             </ul>
                
                <!-- Home Page -->

                <div class="page home-page-background" id="p1">
                  <div class="container-fluid">
                     <li class="icon fa fa-home"><span class="title">Home<span class="hint" style="color:red;"> 



                     </span></span></li>

                  </div>
                </div>
  
                <!-- Events Page -->
                <div class="page" id="p2">
                <div class="container-fluid" id="event-page-style">
                 <!-- iterate through slices of 3 of events, create modals -->
                    <%=@events.order("date").each_slice(3) do |event_row| %>
                      <div class="row">
                        <% event_row.each do |event| %>
                          <div class="col-md-4 homepage-event-image"> 
                          <!-- image is a clickable link to modal of single event page -->
                            <a href="#" class="event-link event-image" data-toggle="modal" data-target="#largeModal<%=event.id%>" style="background: url(<%=event.image%>) center no-repeat; background-size: contain;  background-size: 250px 250px;"> </a> 
                                                              
                                  <!-- Modal -->
                            <div class="modal fade" id="largeModal<%=event.id%>" tabindex="-1" role="dialog" data-width="760" data-backdrop="true">
                              <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
                                <h4 class="modal-title modal-event-title"><%=event.title%></h4>
                              </div>
                                  <div class="modal-body">
                                    <div class="row modal-image">
                                      <div class="col-md-6 container-fluid">
                                      <h5 class="modal-title"> <%=event.description%></h5>
                                      </div>
                                      
                                      <!-- Tinder in modal -->
                                      <div class="col-md-6 container-fluid" style="background-size: 250px 250px; height:300px; color:black;">
                                        <%= link_to 'Pin Event', "/events/#{event.id}/pin_event" %> <br>   

<%    if current_user && current_user.events.include?(event) %>
<%      @shown_user = current_user.get_user(event)   %>
        <% if @shown_user && !@shown_user[1] %>
            <%= link_to "Left", "#", id: 'left'%>
            <script type="text/javascript">
            $('#left').click(function(event) {
              event.preventDefault();
              $.post("/userevents", {
                "shown_user_id" : 1,
                "event_id" : 1,
                "liked" : "no"
              }, function(html) {
                $("#modal-hook").html(html)
                // drop the image im currently showing
                // add a new image, maybe thats returned by the server
                // call render with my new data the server sent me back
              })
                // <% UserEvent.create( user_id: current_user.id, event_id: event.id, shown_user_id: @shown_user[0].id, liked: "no")%>
                //   location.reload(true);
                // $(window).load(function(){
                // $('#largeModal<%=event.id%>').modal('show');
                //   });
            });
              </script>
             <%= link_to "Right", "/events/#{event.id}/users/#{@shown_user[0].id}/tinder/yes", method: :post %><br>
            <%= image_tag(@shown_user_image) %>
            <%= link_to "#{@shown_user[0].name}", "/users/#{@shown_user[0].id}" %><br>
        <% elsif @shown_user %>
<%        @shown_user_image = @shown_user[0].user_image    %>
          <h4 class="modal-title">You are matched with <%=@shown_user[0].name%>!</h3>
           <%= image_tag(@shown_user_image) %><br>
        <% else %>
<%          @message = "There are no Users to display at this time."  %>
             <p class="modal-title" style="height:100%;"><%=@message%></p>
        <% end %>
<%    else %>
<%        @message = "Please pin event to continue."   %>
          <p class="modal-title" style="height:100%;"><%=@message%></p>
<%    end %>                                  
                                      </div>
                                      <!-- end tinder div -->

                                    </div>
                                   <div class="modal-footer">
                                    <button type="button" data-dismiss="modal" class="btn">Close</button>
                                   </div>                                  
                                </div>
                            </div>                              

                            <!-- end modal -->

                            <%= link_to 'Pin Event', "/events/#{event.id}/pin_event" %>   
                          </div>
                        <% end %>
                      </div>
                    <%end%>
                  </div>
                </div>  
  
                <!-- Messages Page --> 
                <div class="page" id="p3">
                  <li class="icon fa fa-comments"><span class="title">Hotline Bling</span></li>
                </div> 
                
                <!-- Footer -->
                <p class="credit"> &copy; Down 2 Go <a href="https://downtogo.com/">Made in Brooklyn</a></p>
          </section>
       
        </div>
      </div>
    </div>
  </div>
