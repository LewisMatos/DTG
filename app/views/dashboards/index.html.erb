.facebook {
    font-family: Roboto;
    height: 150px;
    padding-top: 15vh;
    color: #F7FAFF;
    padding-top: 650px;
    padding-right: 250px;
}

.facebook .spacer {
    margin-bottom: 5px;
}

.facebook .title {
    font-size: 20px;
}

.btn.btn-facebook {
    position: relative;
    color: #F7FAFF;
    text-indent: 35px;
    border-color: #263961;
    background-color: #4c70ba;
    background-image: linear-gradient(to bottom,transparent 0%,#3b5998 65%,#263961 100%);
}

.btn.btn-facebook:before {
    content: '';
    display: block;
    position: absolute;
    width: 35px;
    left: 0;
    top: 0;
    bottom: 0;
    border-right: 1px solid rgba(76,112,186,0.6);
    background-color: #263961;
    background-image: url(https://dl.dropboxusercontent.com/u/48498161/codepen/RNVdYg/facebook-logo.svg);
    background-size: auto 75%;
    background-position: 65% bottom;
    background-repeat: no-repeat;
}

.btn.btn-facebook:hover {
    background-color: #263961;
    background-image: linear-gradient(to bottom,#4c70ba 0%,#3b5998 50%,transparent 100%);
}

.btn.btn-facebook.btn-ocd:before {
    background-position: 50%;
}

.btn.btn-facebook:active,.btn.btn-facebook:focus {
    outline: none;
}

.btn.btn-facebook.btn-xl {
    text-indent: 60px;
}

.btn.btn-facebook.btn-xl:before {
    width: 60px;
}

.btn-xl {
    font-size: 20px;
}

body.modal-open,.modal-open .navbar-fixed-top,.modal-open .navbar-fixed-bottom {
