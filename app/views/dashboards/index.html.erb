.userimage_circle{
 position: relative;
    margin: 0 auto;
    height: 160px;
    width: 160px;
    border: 5px solid #FFFFFF;
    -webkit-border-radius: 500px;
    -moz-border-radius: 500px;
    border-radius: 500px;
    -webkit-box-shadow: 0 0 2px 0 rgba(0,0,0,0.25);
    -moz-box-shadow: 0 0 2px 0 rgba(0,0,0,0.25);
    box-shadow: 0 0 2px 0 rgba(0,0,0,0.25);
    &:after {
      -webkit-border-radius: 500px;
      -moz-border-radius: 500px;
      border-radius: 500px;
      -webkit-box-shadow: 0 5px 8px 0 rgba(0,0,0,0.25) inset;
      -moz-box-shadow: 0 5px 8px 0 rgba(0,0,0,0.25) inset;
      box-shadow: 0 5px 8px 0 rgba(0,0,0,0.25) inset;
      content: "";
      position: absolute;
      left: 0;
      top: 0;
      width: 150px;
      height: 150px;
    }
    img {
      -webkit-border-radius: 500px;
      -moz-border-radius: 500px;
      border-radius: 500px;
      width: 100%;
    }


}
